{
  "required_api_version": "^2.0.0",
  "name": "StarDict Dictionaries Search",
  "description": "Search StarDict dictionaries extension for ulauncher",
  "developer_name": "Prasanna Venkatesh T S",
  "icon": "images/icon.png",
  "options": {
    "query_debounce": 0.05
  },
  "preferences": [
    {
      "id": "search-dictionary",
      "type": "keyword",
      "name": "Search Dictionary",
      "default_value": "sd"
    },
    {
      "id": "dict_path",
      "type": "input",
      "name": "Absolute path to the StarDict dictionary file",
      "description": "Example: '/home/user/dictdata/StarDict English/stardict-english' (without the extension [.dic, .ifo, etc.]).",
      "default_value": ""
    }
  ]
}
